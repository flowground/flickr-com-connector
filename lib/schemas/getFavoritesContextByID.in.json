{
    "type": "object",
    "properties": {
        "api_key": {
            "required": true,
            "type": "string"
        },
        "photo_id": {
            "required": true,
            "type": "string",
            "pattern": "^[0-9]+$"
        },
        "user_id": {
            "required": false,
            "type": "string",
            "pattern": "^([0-9]+@N[0-9]+)|([0-9a-zA-Z-_]+)$"
        }
    }
}